{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Yinyu",
  "version": "0.1.0",
  "identifier": "com.yinyu.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [{ "title": "Yinyu", "width": 1280, "height": 800 }],
    "security": { "csp": null }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true
  }
}
